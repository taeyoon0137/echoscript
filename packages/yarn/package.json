{
  "name": "@echoscript/yarn-plugin",
  "description": "Run echoscript via yarn plugin.",
  "version": "0.0.1-alpha.2.1",
  "scripts": {
    "build": "yarn build:clear && yarn build:types",
    "build:clear": "touch ./dist/temp && rm -rf ./dist/*",
    "build:types": "tsc -b && tsc-alias"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@yarnpkg/core": "^4.0.0-rc.50",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@echoscript/core": "^0.0.1-alpha.2.1",
    "@echoscript/types": "^0.0.1-alpha.2.1"
  },
  "gitHead": "85c7e3570c27949936d194736a1188bafd09a4c9"
}
